<!DOCTYPE html>

<html>
<head>

    <title>@ViewBag.Title</title>
   
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validationEngine.js")" type="text/javascript"></script>
     <script src="@Url.Content("~/Scripts/jquery.validationEngine-en.js")" type="text/javascript"></script> 
    <script src="@Url.Content("~/Scripts/jquery.numeric.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.maskedinput-1.3.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.16.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.ui.mouse.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.ui.dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.ui.position.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.bgiframe-2.1.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.ui.core.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.ui.widget.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.Epi.Survey.Components.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/CheckCodeEngine.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.PreventEnter.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Content/css/epistyle.css")" rel="Stylesheet" type="text/css" />
     <link href="@Url.Content("~/Content/css/page.css")" rel="Stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/validationEngine.jquery.css")" rel="Stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/ui-lightness/jquery-ui-1.8.16.custom.css")" rel="Stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/jquery.ui.all.css")" rel="Stylesheet" type="text/css" />
   
    
   
    
   <script type="text/javascript">

       //debugger;

       $(document).ready(function () {
          
           $('#successContent').hide();


           $("#myform").validationEngine();

           $('div#pages a').click(PostForm);
           function PostForm(event) {
               // Clicking the navigation links at the top, get the target Url and then post the form
               document.forms[0].action = event.target.href;

               $("#myform").submit();

               return false;
           }
       });

 
      
     </script> 
 
      
</head>
<body>
     
     <div id ="container">
            
            <div id="body">
                @RenderBody()
                
            </div>
           
      <div id="footer">
        <div id="footleft">
            <span style="font-size: 8pt;">Powered by:</span>
            <img alt="Epi Info 7" height="42" src="@Url.Content("~/Content/images/epi.png")" width="42" align="middle" />
        </div>
        <div id="footcenter">
        </div>
        <div id="footright">
            <img alt="Centers for Disease Control and Prevention" height="42" src="@Url.Content("~/Content/images/cdclogo.png")" width="71" />
            <img src="@Url.Content("~/Content/images/logo_dhhs.gif")" alt="Department of Health and Human Services" height="42" />
        </div>
        <div style="clear: both;">
        </div>
    </div>
    </div>
     
    
</body>
</html>
