                <RichTextBox Height="41" Margin="25,319,24,0" Name="ExitTextTextBox" VerticalAlignment="Top" VerticalScrollBarVisibility="Visible" Grid.RowSpan="4" />
                <Button Content="." Height="66" HorizontalAlignment="Left" Margin="557,14,0,0" Name="ClearExitTextButton" VerticalAlignment="Top" Width="12" Grid.Row="3" Click="ClearExitTextButton_Click" Grid.RowSpan="2" />
