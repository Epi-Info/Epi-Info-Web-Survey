                <Label Content="Date Completed" Height="28" HorizontalAlignment="Left" Margin="292,52,0,0" Name="label4" VerticalAlignment="Top" />
                <DatePicker Height="50" HorizontalAlignment="Left" Margin="302,74,0,0" Name="datePicker1" VerticalAlignment="Top" Width="140" />
